{
  "name": "simple-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "dev:https": "NODE_ENV=development node server.js",
    "build": "rm -rf .next && rm -rf out && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:cross-browser": "scripts/run-cross-browser-tests.sh",
    "test:cross-browser:desktop": "scripts/run-cross-browser-tests.sh desktop",
    "test:cross-browser:mobile": "scripts/run-cross-browser-tests.sh mobile",
    "test:cross-browser:compatibility": "scripts/run-cross-browser-tests.sh compatibility",
    "test:cross-browser:maps": "scripts/run-cross-browser-tests.sh maps",
    "test:e2e": "playwright test tests/e2e/",
    "test:ci": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:security": "playwright test --project=security",
    "security:audit": "npm audit && npm audit fix",
    "security:audit:prod": "npm audit --production",
    "security:deps": "npx snyk test",
    "security:lint": "next lint",
    "security:check": "npm run security:audit && npm run security:lint",
    "security:test": "npx eslint --no-eslintrc -c .eslintrc.security.json 'tests/**/*.{js,ts,tsx}'",
    "security:secrets": "npx secretlint \"tests/**/*.{js,ts,tsx}\"",
    "security:all": "npm run security:test && npm run security:secrets && npm run test:security",
    "verify-env": "node scripts/verify-deploy-env.js",
    "prebuild": "npm run verify-env"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-toast": "1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "0.0.13",
    "@react-email/tailwind": "1.0.4",
    "@react-google-maps/api": "2.20.6",
    "@stripe/react-stripe-js": "^3.4.0",
    "@stripe/stripe-js": "^6.1.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tsparticles/engine": "3.8.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.8.1",
    "@types/stripe": "^8.0.417",
    "aos": "^3.0.0-beta.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "^12.4.7",
    "isomorphic-dompurify": "2.22.0",
    "lord-icon-element": "^4.1.2",
    "lottie-web": "5.12.2",
    "lucide-react": "0.475.0",
    "mini-svg-data-uri": "1.4.4",
    "next": "15.3.0",
    "node-fetch": "3.3.2",
    "react": "19.0.0",
    "react-day-picker": "9.6.7",
    "react-dom": "19.0.0",
    "react-fast-marquee": "1.6.5",
    "react-icons": "5.5.0",
    "resend": "4.1.2",
    "simplex-noise": "4.0.3",
    "stripe": "^17.7.0",
    "swiper": "11.2.6",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.3",
    "@playwright/test": "^1.50.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@secretlint/secretlint-rule-preset-recommend": "^7.0.9",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/aos": "^3.0.7",
    "@types/jest": "29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "10.4.20",
    "date-fns": "^4.1.0",
    "eslint": "9.21.0",
    "eslint-config-next": "^14.2.24",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-xss": "^0.1.12",
    "https": "1.0.0",
    "jest": "29.7.0",
    "local-ssl-proxy": "2.0.5",
    "lockfile-lint": "^4.13.2",
    "postcss": "^8.5.3",
    "rollup": "^4.12.0",
    "rollup-plugin-postcss": "^4.0.2",
    "secretlint": "^7.0.9",
    "snyk": "1.1296.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3"
  }
}
