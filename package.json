{
  "name": "simple-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev --hostname 0.0.0.0",
    "dev:https": "NODE_ENV=development node server.js",
    "build": "rm -rf .next && rm -rf out && next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:cross-browser": "scripts/run-cross-browser-tests.sh",
    "test:cross-browser:desktop": "scripts/run-cross-browser-tests.sh desktop",
    "test:cross-browser:mobile": "scripts/run-cross-browser-tests.sh mobile",
    "test:cross-browser:compatibility": "scripts/run-cross-browser-tests.sh compatibility",
    "test:cross-browser:maps": "scripts/run-cross-browser-tests.sh maps",
    "test:e2e": "playwright test tests/e2e/",
    "test:ci": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:security": "playwright test --project=security",
    "security:audit": "npm audit && npm audit fix",
    "security:audit:prod": "npm audit --production",
    "security:deps": "npx snyk test",
    "security:lint": "next lint",
    "security:check": "npm run security:audit && npm run security:lint",
    "security:test": "npx eslint --no-eslintrc -c .eslintrc.security.json 'tests/**/*.{js,ts,tsx}'",
    "security:secrets": "npx secretlint \"tests/**/*.{js,ts,tsx}\"",
    "security:all": "npm run security:test && npm run security:secrets && npm run test:security",
    "verify-env": "node scripts/verify-deploy-env.js",
    "verify-maps": "node scripts/verify-google-maps.js",
    "prebuild": "npm run verify-env && npm run verify-maps"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@next/third-parties": "14.2.28",
    "@radix-ui/react-checkbox": "1.1.5",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-tooltip": "1.1.8",
    "@react-email/components": "0.0.36",
    "@react-email/tailwind": "1.0.4",
    "@react-google-maps/api": "2.19.3",
    "@stripe/react-stripe-js": "3.4.0",
    "@stripe/stripe-js": "3.0.10",
    "@supabase/supabase-js": "2.39.8",
    "@types/stripe": "8.0.417",
    "@vercel/analytics": "1.2.2",
    "@vis.gl/react-google-maps": "1.5.2",
    "aos": "3.0.0-beta.6",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "critters": "0.0.23",
    "framer-motion": "11.0.20",
    "isomorphic-dompurify": "2.4.0",
    "lord-icon-element": "4.1.2",
    "lottie-web": "5.12.2",
    "lucide-react": "0.359.0",
    "mini-svg-data-uri": "1.4.4",
    "next": "14.2.28",
    "node-fetch": "3.3.2",
    "react": "18.2.0",
    "react-day-picker": "8.10.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "1.6.4",
    "react-icons": "5.0.1",
    "react-tsparticles": "2.12.0",
    "resend": "3.2.0",
    "sharp": "0.34.1",
    "simplex-noise": "4.0.1",
    "snyk": "1.1296.2",
    "stripe": "14.22.0",
    "swiper": "11.0.7",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7",
    "tsparticles-engine": "2.12.0",
    "tsparticles-slim": "2.12.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.28",
    "@next/eslint-plugin-next": "14.2.28",
    "@playwright/test": "1.42.1",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@secretlint/secretlint-rule-preset-recommend": "5.2.4",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/aos": "3.0.7",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "date-fns": "3.6.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.28",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "2.1.1",
    "eslint-plugin-xss": "0.1.12",
    "https": "1.0.0",
    "image-webpack-loader": "8.1.0",
    "jest": "29.7.0",
    "lint-staged": "15.5.1",
    "local-ssl-proxy": "2.0.5",
    "lockfile-lint": "4.13.2",
    "next-pwa": "5.6.0",
    "postcss": "8.4.38",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "rollup": "4.40.0",
    "rollup-plugin-postcss": "4.0.2",
    "secretlint": "5.2.4",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.3"
  }
}
